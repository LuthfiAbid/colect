<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.android.collect">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.hardware.camera"/>
    <uses-permission android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:name=".KtAplication"
            android:hardwareAccelerated="true"
            android:allowBackup="true"
            android:icon="@drawable/icon"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".useractivity.EditUserProfile">
        </activity>
        <activity
                android:name=".useractivity.ProfileUserActivity"
                android:parentActivityName=".useractivity.MainUserActivity">
        </activity>
        <activity
                android:name=".useractivity.MainUserActivity">
        </activity>
        <activity android:name=".kasiractivity.FinalTransaction">
        </activity>
        <activity android:name=".kasiractivity.AddTransaction">
        </activity>
        <activity
                android:name=".kasiractivity.EditProfile"
                android:parentActivityName=".kasiractivity.ProfileActivity">
        </activity>
        <activity
                android:name=".kasiractivity.ProfileActivity"
                android:parentActivityName=".kasiractivity.MainActivity">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity
                android:name=".kasiractivity.MainActivity"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".kasiractivity.Login">
        </activity>
    </application>

</manifest>